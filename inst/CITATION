citHeader("To cite bycatch in publications use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{bycatch}: Estimation and expanded bycatch estimation using Bayesian models",
         author = c(person("Eric J.", "Ward"),
                    person("Jason. E.", "Jannot")),
         year = year,
         note = note,
         url = "https://github.com/ericward-noaa/bycatch")
